{
    "Minifiers": {
        "GZip": false,
        "Enabled": true,
        "Css": {
            "CommentMode": "Important",
            "ColorNames": "Hex",
            "TermSemicolons": true,
            "OutputMode": "SingleLine",
            "IndentSize": 4
        },
        "Javascript": {
            "RenameLocals": true,
            "PreserveImportantComments": true,
            "EvalTreatment": "Ignore",
            "TermSemicolons": true,
            "OutputMode": "SingleLine",
            "IndentSize": 4
        }
    },
    "Autoprefix": {
        "Enabled": true,
        "ProcessingOptions": {
            "Browsers": [
                "last 4 versions"
            ],
            "Cascade": true,
            "Add": true,
            "Remove": true,
            "Supports": true,
            "Flexbox": "All",
            "Grid": "None",
            "IgnoreUnknownVersions": false,
            "Stats": "",
            "SourceMap": true,
            "InlineSourceMap": false,
            "SourceMapIncludeContents": false,
            "OmitSourceMapUrl": false
        }
    },
    "CompilerSettings": {
        "Sass": {
            "IndentType": "Space",
            "IndentWidth": 4,
            "OutputStyle": "Expanded",
            "RelativeUrls": true,
            "LineFeed": "Lf",
            "SourceMap": false
        }
    },
    "Output": {
        "Preserve": true
    }
}